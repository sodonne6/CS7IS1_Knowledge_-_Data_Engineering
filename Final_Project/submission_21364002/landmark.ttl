@prefix ex:   <http://example.org/flight#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

######## Landmark Types ########
ex:Museum a ex:LandmarkType ; rdfs:label "Museum" .
ex:Monument a ex:LandmarkType ; rdfs:label "Monument" .
ex:Bridge a ex:LandmarkType ; rdfs:label "Bridge" .
ex:ReligiousSite a ex:LandmarkType ; rdfs:label "Religious Site" .
ex:Square a ex:LandmarkType ; rdfs:label "Square" .
ex:District a ex:LandmarkType ; rdfs:label "District" .
ex:Park a ex:LandmarkType ; rdfs:label "Park" .
ex:Architecture a ex:LandmarkType ; rdfs:label "Architecture" .

######## FRANCE ########

<http://example.org/flight/city/Paris_France> ex:hasLandmark
  ex:eiffel_tower, ex:louvre, ex:notre_dame .

ex:eiffel_tower a ex:Landmark ; rdfs:label "Eiffel Tower" ; ex:hasLandmarkType ex:Monument .
ex:louvre a ex:Landmark ; rdfs:label "Louvre Museum" ; ex:hasLandmarkType ex:Museum .
ex:notre_dame a ex:Landmark ; rdfs:label "Notre-Dame Cathedral" ; ex:hasLandmarkType ex:ReligiousSite .

